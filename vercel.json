{
    "builds": [
      {
        "src": "my-app/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "api/weather.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/weather.py"
      },
      {
        "src": "/(.*)",
        "dest": "my-app/build/$1"
      }
    ]
  }
  